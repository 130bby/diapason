<h1>FormationElement</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Etablissement</th>
            <td>{{ entity.etablissement }}</td>
        </tr>
        <tr>
            <th>Diplome</th>
            <td>{{ entity.diplome }}</td>
        </tr>
        <tr>
            <th>Annee</th>
            <td>{{ entity.annee }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('formationelement') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('formationelement_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('formationelement_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
